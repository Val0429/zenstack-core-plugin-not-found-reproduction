{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/user/(.*)": "<rootDir>/../libs/user/src/$1",
    "@app/user": "<rootDir>/../libs/user/src",
    "@app/app-host/(.*)": "<rootDir>/../libs/app-host/src/$1",
    "@app/app-host": "<rootDir>/../libs/app-host/src",
    "@app/swagger/(.*)": "<rootDir>/../libs/swagger/src/$1",
    "@app/swagger": "<rootDir>/../libs/swagger/src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1",
    "@app/prisma": "<rootDir>/../libs/prisma/src"
  }
}